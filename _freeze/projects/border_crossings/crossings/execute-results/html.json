d_pal()(length(unique(crossings24$most_common))),\n  domain = crossings24$most_common\n)\n\n# build the map\nleaflet(crossings24) |>\n  addTiles() |>\n  addCircleMarkers(\n    lng = ~longitude,\n    lat = ~latitude,\n    # set a minimum circle size of 3 (helps with legibility)\n    radius = ~pmax(sqrt(crossings) / 200, 3),\n    fill = TRUE,\n    stroke = FALSE,\n    fillOpacity = 0.7, \n    popup = ~paste0(\n      \"<strong>Port: </strong>\", port_name,\n      \"<br><strong>Most Common Method: </strong>\", most_common,\n      \"<br><strong>Total Crossings (2024): </strong>\", scales::comma(crossings)\n    ),\n    color = ~color_palette(most_common)\n  ) |>\n  addLegend(\n    \"bottomright\",\n    pal = color_palette,\n    values = ~most_common,\n    title = \"Most Common Method of Crossing\"\n  )\n```\n\n::: {.cell-output-display}\n\n```{=html}\n<div class=\"leaflet html-widget html-fill-item\" id=\"htmlwidget-1f3cecf15b883d2b9028\" style=\"width:100%;height:464px;\"></div>\n<script type=\"application/json\" data-for=\"htmlwidget-1f3cecf15b883d2b9028\">{\"x\":{\"options\":{\"crs\":{\"crsClass\":\"L.CRS.EPSG3857\",\"code\":null,\"proj4def\":null,\"projectedBounds\":null,\"options\":{}}},\"calls\":[{\"method\":\"addTiles\",\"args\":[\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\",null,null,{\"minZoom\":0,\"maxZoom\":18,\"tileSize\":256,\"subdomains\":\"abc\",\"errorTileUrl\":\"\",\"tms\":false,\"noWrap\":false,\"zoomOffset\":0,\"zoomReverse\":false,\"opacity\":1,\"zIndex\":1,\"detectRetina\":false,\"attribution\":\"&copy; <a href=\\\"https://openstreetmap.org/copyright/\\\">OpenStreetMap<\\/a>,  <a href=\\\"https://opendatacommons.org/licenses/odbl/\\\">ODbL<\\/a>\"}]},{\"method\":\"addCircleMarkers\",\"args\":[[44.859,45.806,45.569,46.135,46.765,47.16,47.36,47.249,44.4,45.189,46.925,46.45,45.012,45.013,45.006,45.011,45.015,44.733,44.991,44.135,44.347,45.009,44.992,43.095,25.952,29.327,28.705,27.5,26.095,26.366,26.062,26.404,31.673,31.764,29.562,31.435,31.784,3